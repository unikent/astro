{
  "private": true,
  "name": "Astro",
  "description": "Super slick site editor",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack --progress --colors --hide-modules --display-error-details",
    "watch": "cross-env NODE_ENV=development webpack --progress --watch --colors --hide-modules",
    "hot": "cross-env NODE_ENV=development webpack-dev-server --hot --colors",
    "production": "cross-env NODE_ENV=production webpack --progress --colors --hide-modules",
    "refresh": "php artisan migrate:refresh && php artisan db:seed",
    "unit": "mocha-webpack --webpack-config resources/assets/js/test/webpack.config-test.babel.js \"resources/assets/js/test/unit/*.js\"",
    "validate": "node node_modules/astro-theme-validator/console",
    "eslint": "node_modules/.bin/eslint resources/assets/js/**/*.{js,vue} --format codeframe",
    "postinstall": "echo 'Installing assets' && mkdir -p public/kent-theme-assets && cp -R node_modules/kent-theme-assets/public/assets public/kent-theme-assets/"
  },
  "dependencies": {
    "@tinymce/tinymce-vue": "^2.0.0",
    "astro-theme-validator": "git+ssh://git@gitlab.kent.ac.uk:webdev/astro-theme-validator",
    "async-validator": "^1.8.1",
    "axios": "^0.17.1",
    "bulma": "^0.6.1",
    "chart.js": "^2.7.1",
    "color-thief-standalone": "^1.0.3",
    "element-ui": "^2.0.5",
    "flat": "^4.0.0",
    "imagesloaded": "^4.1.2",
    "kent-font": "git+ssh://git@gitlab.kent.ac.uk/webdev/kent-font.git#master",
    "kent-theme-assets": "git+ssh://git@gitlab.kent.ac.uk:webdev/kent-theme-assets",
    "lodash": "^4.17.4",
    "normalize.css": "^7.0.0",
    "scribe-editor": "^3.3.0",
    "scribe-plugin-formatter-plain-text-convert-new-lines-to-html": "^0.1.1",
    "scribe-plugin-heading-command": "^0.1.0",
    "scribe-plugin-inline-styles-to-elements": "^0.1.0",
    "scribe-plugin-link-prompt-command": "^1.0.0",
    "scribe-plugin-sanitizer": "^0.1.10",
    "tinymce": "^5.0.3",
    "underscore.string": "^3.3.4",
    "vue": "^2.5.3",
    "vue-chartjs": "^3.0.0",
    "vue-router": "^3.0.1",
    "vuedraggable": "^2.14.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@epegzz/sass-vars-loader": "^3.2.0",
    "ajv": "^5.3.0",
    "axios-mock-adapter": "^1.8.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^8.0.2",
    "babel-loader": "^7.0.0",
    "babel-plugin-array-includes": "^2.0.3",
    "babel-plugin-transform-class-properties": "^6.22.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.22.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.3.2",
    "chai": "^4.0.2",
    "chalk": "^2.3.0",
    "copy-webpack-plugin": "^4.6.0",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.1",
    "dotenv": "^4.0.0",
    "eslint": "^4.12.1",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-html": "^4.0.1",
    "eslint-plugin-vue": "^2.0.1",
    "external-svg-sprite-loader": "^3.3.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "html-loader": "^0.5.5",
    "jsdom": "^11.12.0",
    "mocha": "^4.0.1",
    "mocha-webpack": "1.0.1",
    "node-sass": "^4.5.3",
    "resolve-url-loader": "^2.0.2",
    "sass-loader": "^6.0.5",
    "sinon": "^4.1.2",
    "style-loader": "^0.19.0",
    "svg-sprite-loader": "^3.4.1",
    "vue-loader": "^13.5.0",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.5.3",
    "webpack": "3.9.0",
    "webpack-dev-server": "^3.1.11",
    "webpack-notifier": "^1.5.0"
  }
}
